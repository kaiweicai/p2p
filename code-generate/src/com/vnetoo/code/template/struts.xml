<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="${baseName}" extends="default">
		<action name="${baseName}"
			class="${action}">
			<result name="LIST">/jsp/${baseName.lowerFirst}/${baseName}List.jsp</result>
			<result name="TABLE">/jsp/${baseName.lowerFirst}/${baseName}Table.jsp</result>
			<result name="CREATE">/jsp/${baseName.lowerFirst}/Create${baseName}.jsp</result>
		</action>
	</package>
</struts>